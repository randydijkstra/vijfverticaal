{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 2,

  "name": "VijfVerticaal",
  "short_name": "VijfVerticaal",
  "description": "Prototype chrome-extensie voor ad.nl.",
 
  "version": "1.0.0",
	"icons": { 
		"48": "images/logo-48x48.png",
		"128": "images/logo-128x128.png" 
	},					
	"permissions": [
		"http://*.ad.nl/*",
		"https://*.ad.nl/*"
    ],
	"content_scripts": [
	    {
	      "matches": [
				"http://*.ad.nl/*",
				"https://*.ad.nl/*"
		  ],
	      "css": ["css/style_injector.css"],		  
	      "js": ["js/library/jquery-1.11.1.min.js", "js/GameEngine.js", "js/content_injector.js"],
	      "run_at": "document_end",
	      "all_frames": false
	    }
	],
	"web_accessible_resources": [
    	"images/*.png",
    	"js/library/*.*",
    	"js/*.*",
    	"*.*"
	]
}